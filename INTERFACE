interface Addition {
    void add(int a, int b);
}

class Calculator implements Addition {
    public void add(int a, int b) {
        int sum = a + b;
        System.out.println("Sum: " + sum);
    }
}

public class Main {
    public static void main(String[] args) {
        Calculator s1 = new Calculator();
        s1.add(10, 20);
    }
}
